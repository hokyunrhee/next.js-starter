    "postinstall-postinstall": "^2.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.12.2",
    "react-query": "^3.19.6"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^1.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/node": "^16.6.1",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
